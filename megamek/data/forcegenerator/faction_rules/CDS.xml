<?xml version="1.0" encoding="utf-8"?>

<!--Clan Sea Fox/Diamond Shark uses fewer galaxies and fewer clusters per
galaxy than most Clans, but more trinaries per cluster. After a major
reorganization and return to the original name, galaxies are replaced
with khanates and clusters with aimags. Though they have greater
independence than galaxies and clusters, for the purposes of force
generation the difference is simply one of nomenclature. Each khanate
is commanded by a saKhan (except the ilKhanate, commanded by the khan),
which makes the post-3100 rank of saKhan equivalent to galaxy commander,
but with more independence.-->

<ruleset faction="CDS" ratingSystem="CLAN" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="formationRulesetSchema.xsd">

	<defaults>
		<eschelon>%CLUSTER%</eschelon>
		<rankSystem ifDateBetween="3100,">%RANK_CUSTOM%</rankSystem>
		<rankSystem>%RANK_CLAN%</rankSystem>
		<rating>FL</rating>
	</defaults>
	
	<customRanks>
	    <base>%RANK_CLAN%</base>
		<rank>%STAR_COL%:ovKhan,--MW,--MW,--MW,--MW,-</rank>
	</customRanks>
	
	<toc>
		<unitType>
			<option ifDateBetween="3100,">null,Mek,Tank,VTOL,Infantry,BattleArmor,Aero,Conventional Fighter,Dropship,Jumpship</option>
			<option ifDateBetween="2870,3099">null,Mek,Tank,VTOL,Infantry,BattleArmor,Aero,Conventional Fighter,Warship,Dropship,Jumpship</option>
			<option ifDateBetween=",2869">null,Mek,Tank,VTOL,Infantry,Aero,Conventional Fighter,Warship,Dropship,Jumpship</option>
		</unitType>
		
		<eschelon>
			<option ifUnitType="">%TOUMAN%,%GALAXY%,%CLUSTER%</option>
			<option ifUnitType="Warship|Dropship|Jumpship">%STAR%</option>
			<option ifUnitType="Mek" ifDateBetween="2870,">%TRINARY%^,%TRINARY%,%BINARY%^,%BINARY%,%STAR%^,%STAR%</option>
			<option>%TRINARY%,%BINARY%,%STAR%</option>
		</eschelon>
		
		<rating>
			<option ifUnitType="Tank|VTOL">SL:Spina,PG:Garrison,Sol:Solahma</option>
			<option ifUnitType="Infantry" ifDateBetween="2900,">PG:Garrison,Sol:Solahma</option>
			<option ifUnitType="Infantry">PG:Garrison,Sol:Solahma</option>
			<option ifEschelon="%TOUMAN%"/>
			<option ifEschelon="%GALAXY%" ifDateBetween="3100,"/>
			<option ifEschelon="%CLUSTER%">FL:Front Line,SL:Spina,PG:Garrison</option>
			<option>Keshik,FL:Front Line,SL:Spina,PG:Garrison,Sol:Solahma</option>
		</rating>
		
		<flags>
			<option ifEschelon="%CLUSTER%">command:Command Cluster,cruiser:Cruiser Cluster,combined:Combined Cluster,airAssault:Air Assault Cluster</option>
		</flags>
	</toc>
	
	<force eschelon="%TOUMAN%" eschName="Touman">
		<co>%KHAN%</co>
		<xo ifDateBetween=",3099">%SAKHAN%</xo>
		
		<subforces>
			<subforce rating="FL" ifDateBetween=",3099"
			    role="command" flags="alpha"
				name="Alpha Galaxy">%GALAXY%</subforce>
			<subforce rating="FL" ifDateBetween=",3099"
			    flags="beta"
				name="Beta Galaxy">%GALAXY%</subforce>
			<subforce rating="FL" ifDateBetween=",3099"
			    flags="gamma"
				name="Gamma Galaxy">%GALAXY%</subforce>
			<subforce rating="SL" ifDateBetween=",3099"
			    flags="lambda"
				name="Lambda Spina Galaxy">%GALAXY%</subforce>
			<subforce rating="SL" ifDateBetween=",3075"
			    flags="rho"
				name="Rho Spina Galaxy">%GALAXY%</subforce>
			<subforce rating="SL" ifDateBetween=",3099"
			    flags="omega"
				name="Omega Galaxy">%GALAXY%</subforce>
			<subforce rating="SL" ifDateBetween=",3099"
			    flags="sigma"
				name="Sigma Galaxy">%GALAXY%</subforce>
			<subforce rating="SL" ifDateBetween=",3099"
			    flags="zeta"
				name="Zeta Galaxy">%GALAXY%</subforce>
				
			<subforce name="Spectral Diamond Naval Star"
				flags="+navalFluff" unitType="Warship" ifDateBetween=",3099"
				>%STAR%</subforce>
			<subforce name="Black Diamond Naval Star"
				flags="+navalFluff" unitType="Warship" ifDateBetween=",3099"
				>%STAR%</subforce>
			<subforce name="Blue Diamond Naval Star"
				flags="+navalFluff" unitType="Warship" ifDateBetween=",3099"
				>%STAR%</subforce>
				
			<subforce ifDateBetween="3100," role="command"
				name="ilKhanate">%GALAXY%</subforce>
			<subforce ifDateBetween="3100,"
				name="Skate Khanate">%GALAXY%</subforce>
			<subforce ifDateBetween="3100,"
				name="Fox Khanate">%GALAXY%</subforce>
			<subforce ifDateBetween="3100,"
				name="Spina Khanate">%GALAXY%</subforce>
			<subforce ifDateBetween="3100,"
				name="Tiburon Khanate">%GALAXY%</subforce>
		</subforces>
	</force>
	
	<force eschelon="%GALAXY%" eschName="Khanate" ifDateBetween="3100,">
		<name>{greek} Khanate</name>
		<co>%SAKHAN%</co>
		
		<subforces>
		    <subforce ifName="ilKhanate" role="command"
		        fluffName="CSF Tsunami" unitType="Warship"
				chassis="Potemkin Troop Cruiser">%ELEMENT%</subforce>
		    <subforce ifName="Skate Khanate" role="command"
		        fluffName="CSF Atlantean" unitType="Warship"
				chassis="Potemkin Troop Cruiser">%ELEMENT%</subforce>
		    <subforce ifName="Fox Khanate" role="command"
		        fluffName="CSF Kraken" unitType="Warship"
				chassis="Potemkin Troop Cruiser">%ELEMENT%</subforce>
		    <subforce ifName="Spina Khanate" role="command"
		        fluffName="CSF Poseidon" unitType="Warship"
				chassis="Potemkin Troop Cruiser">%ELEMENT%</subforce>
		    <subforce ifName="Tiburon Khanate" role="command"
		        fluffName="CSF Titanic" unitType="Warship"
				chassis="Potemkin Troop Cruiser">%ELEMENT%</subforce>
			<subforce num="5" rating="FL">%CLUSTER%</subforce>
		</subforces>
	</force>
	
	<force eschelon="%GALAXY%" eschName="Galaxy">
		<name>{greek} Galaxy</name>
		<co>%GALAXY_CMDR%</co>
		
		<!-- All galaxies have a command cluster and a cruiser cluster,
		and most galaxies have a combined cluster. FL galaxies have a
		fourth cluster while Spina galaxies have only the three. Garrison
		galaxies have a fifth cluster which is often an air assault
		cluster.-->
		
		<subforces>
			<subforce role="command">%CLUSTER%</subforce>
			<subforce ifRating="FL|PG">%CLUSTER%</subforce>
			<subforceOption>
				<option flags="combined" weight="2">%CLUSTER%</option>
				<option weight="1">%CLUSTER%</option>
			</subforceOption>
			<subforce flags="cruiser" role="cavalry">%CLUSTER%</subforce>
			<subforceOption ifRating="PG">
				<option flags="airAssault" weight="2">%CLUSTER%</option>
				<option weight="1">%CLUSTER%</option>
			</subforceOption>
		</subforces>
	</force>
	
	<!-- Air assault clusters, found in some garrison clusters, are
	comprised of three trinaries of ASFs and either one BA trinary or
	one supernova trinary.-->
	
	<force eschelon="%CLUSTER%" eschName="Cluster" ifFlags="airAssault">
		<name>Air Assault Cluster</name>
		<co>%STAR_COL%</co>
		
		<weightClass>
			<option weight="1">H</option>
			<option weight="1">M</option>
			<option weight="1">L</option>
		</weightClass>
				
		<subforces>
			<subforceOption ifWeightClass="H">
				<option weightClass="H,H,H" unitType="Aero">%TRINARY%</option>
				<option weightClass="H,H,M" unitType="Aero">%TRINARY%</option>
			</subforceOption>
			<subforceOption ifWeightClass="M">
				<option weightClass="H,M,L" unitType="Aero" weight="3">%TRINARY%</option>
				<option weightClass="H,L,L" unitType="Aero">%TRINARY%</option>
				<option weightClass="M,M,M" unitType="Aero" weight="2">%TRINARY%</option>
				<option weightClass="M,M,L" unitType="Aero" weight="2">%TRINARY%</option>
			</subforceOption>
			<subforceOption ifWeightClass="L">
				<option weightClass="M,L,L" unitType="Aero">%TRINARY%</option>
				<option weightClass="L,L,L" unitType="Aero">%TRINARY%</option>
			</subforceOption>
		</subforces>

		<subforces ifDateBetween="2900,">
			<subforceOption ifWeightClass="H|A">
				<option weightClass="A" unitType="Mek" ifRating="FL|Keshik"
					augmented="1" weight="2">%TRINARY%</option>
				<option weightClass="H" unitType="Mek" ifRating="FL|Keshik"
					augmented="1" weight="3">%TRINARY%</option>
				<option weightClass="M" unitType="Mek" ifRating="FL|Keshik"
					augmented="1" weight="2">%TRINARY%</option>
				<option unitType="BattleArmor" weight="7">%TRINARY%</option>
			</subforceOption>
			<subforceOption ifWeightClass="M">
				<option weightClass="H" unitType="Mek" ifRating="FL|Keshik"
					augmented="1" weight="2">%TRINARY%</option>
				<option weightClass="M" unitType="Mek" ifRating="FL|Keshik"
					augmented="1" weight="3">%TRINARY%</option>
				<option weightClass="L" unitType="Mek" ifRating="FL|Keshik"
					augmented="1" weight="2">%TRINARY%</option>
				<option unitType="BattleArmor" weight="7">%TRINARY%</option>
			</subforceOption>
			<subforceOption ifWeightClass="L">
				<option weightClass="M" unitType="Mek" ifRating="FL|Keshik"
					augmented="1" weight="1">%TRINARY%</option>
				<option weightClass="L" unitType="Mek" ifRating="FL|Keshik"
					augmented="1" weight="2">%TRINARY%</option>
				<option unitType="BattleArmor" weight="3">%TRINARY%</option>
			</subforceOption>
		</subforces>
	</force>
	
	<force eschelon="%CLUSTER%" eschName="Cluster">
		<name ifDateBetween="3100,">{greek} Aimag</name>
		<name ifRole="command" ifFlags="alpha">The Diamond Skate</name>
		<name ifRole="command" ifFlags="beta">The Pearl Skate</name>
		<name ifRole="command" ifFlags="gamma">The Emerald Skate</name>
		<name ifRole="command" ifFlags="omega">The Coral Skate</name>
		<name ifRole="command" ifFlags="lambda">The Sapphire Skate</name>
		<name ifRole="command" ifFlags="rho">The Opal Skate</name>
		<name ifRole="command" ifFlags="sigma">The Moonstone Skate</name>
		<name ifRole="command" ifFlags="zeta">The Agate Skate</name>
		<name ifRole="command">Command Cluster</name>
		<name ifFlags="cruiser">Cruiser Cluster</name>
		<name ifFlags="airAssault">Air Assault Cluster</name>
		<name ifFlags="combined"
			ifWeightClass="A|H">Combined Assault Cluster</name>
		<name ifFlags="combined"
			ifWeightClass="M">Combined Battle Cluster</name>
		<name ifFlags="combined"
			ifWeightClass="L">Combined Strike Cluster</name>
		<name ifWeightClass="A|H">Assault Cluster</name>
		<name ifWeightClass="M">Battle Cluster</name>
		<name ifWeightClass="L">Strike Cluster</name>
		<co ifDateBetween="3100," title="ovKhan">%STAR_COL%</co>
		<co>%STAR_COL%</co>
		
		<!-- The UI can set a command flag; convert to role -->
		<role ifFlags="command">
			<option>command</option>
		</role>
		
		<weightClass>
			<option weight="2">H</option>
			<option weight="2">M</option>
			<option weight="1">L</option>
			<option ifRole="command">H</option>
			<option ifRole="command">M</option>
		</weightClass>
		
		<!-- FL and Spina clusters typically have six trinaries, and
		nearly always include a combination of Meks, BA, and ASFs.
		Garrison clusters usually have four trinaries. All clusters are
		assumed here to have at least two trinaries of Meks, one of BA,
		and one of ASFs. FL and Spina have two additional clusters,
		one of which is either Mek or ASF, and the other of which is
		Mek or BA, or in combined clusters another ASF trinary. Though
		most trinaries are pure, FL clusters have a command trinary
		consisting of a command star and a support binary. Though the
		makeup of the support binary is not detailed, it seems likely
		that it can be mixed.-->
		
		<subforces>
			<!-- Command trinary -->
			<subforceOption ifWeightClass="A">
				<option weightClass="A" unitType="Mek"
					role="command" weight="2">%TRINARY%</option>
				<option weightClass="H" unitType="Mek"
					role="command" weight="1">%TRINARY%</option>
			</subforceOption>
			<subforceOption ifWeightClass="H">
				<option weightClass="A" unitType="Mek"
					role="command" weight="2">%TRINARY%</option>
				<option weightClass="H" unitType="Mek"
					role="command" weight="3">%TRINARY%</option>
				<option weightClass="M" unitType="Mek"
					role="command" weight="2">%TRINARY%</option>
			</subforceOption>
			<subforceOption ifWeightClass="M">
				<option weightClass="H" unitType="Mek"
					role="command" weight="2">%TRINARY%</option>
				<option weightClass="M" unitType="Mek"
					role="command" weight="3">%TRINARY%</option>
				<option weightClass="L" unitType="Mek"
					role="command" weight="2">%TRINARY%</option>
			</subforceOption>
			<subforceOption ifWeightClass="L">
				<option weightClass="M" unitType="Mek"
					role="command" weight="1">%TRINARY%</option>
				<option weightClass="L" unitType="Mek"
					role="command" weight="2">%TRINARY%</option>
			</subforceOption>

			<!-- The second trinary is often a supernova trinary. -->
			<subforceOption ifWeightClass="A">
				<option weightClass="A" unitType="Mek"
					weight="2">%TRINARY%</option>
				<option weightClass="H" unitType="Mek"
					weight="1">%TRINARY%</option>
				<option weightClass="A" unitType="Mek" augmented="1"
					ifDateBetween="2870," ifRating="FL|Keshik"
					weight="4">%TRINARY%</option>
				<option weightClass="H" unitType="Mek" augmented="1"
					ifDateBetween="2870," ifRating="FL|Keshik"
					weight="2">%TRINARY%</option>
				<option weightClass="A" unitType="Mek" augmented="1"
					ifDateBetween="2900," ifRating="SL"
					weight="2">%TRINARY%</option>
				<option weightClass="H" unitType="Mek" augmented="1"
					ifDateBetween="2900," ifRating="SL"
					weight="1">%TRINARY%</option>
			</subforceOption>
			<subforceOption ifWeightClass="H">
				<option weightClass="A" unitType="Mek"
					weight="2">%TRINARY%</option>
				<option weightClass="H" unitType="Mek"
					weight="3">%TRINARY%</option>
				<option weightClass="M" unitType="Mek"
					weight="2">%TRINARY%</option>
				<option weightClass="A" unitType="Mek" augmented="1"
					ifDateBetween="2870," ifRating="FL|Keshik"
					weight="4">%TRINARY%</option>
				<option weightClass="H" unitType="Mek" augmented="1"
					ifDateBetween="2870," ifRating="FL|Keshik"
					weight="6">%TRINARY%</option>
				<option weightClass="M" unitType="Mek" augmented="1"
					ifDateBetween="2870," ifRating="FL|Keshik"
					weight="4">%TRINARY%</option>
				<option weightClass="A" unitType="Mek" augmented="1"
					ifDateBetween="2900," ifRating="SL"
					weight="2">%TRINARY%</option>
				<option weightClass="H" unitType="Mek" augmented="1"
					ifDateBetween="2900," ifRating="SL"
					weight="3">%TRINARY%</option>
				<option weightClass="M" unitType="Mek" augmented="1"
					ifDateBetween="2900," ifRating="SL"
					weight="2">%TRINARY%</option>
			</subforceOption>
			<subforceOption ifWeightClass="M">
				<option weightClass="H" unitType="Mek"
					weight="2">%TRINARY%</option>
				<option weightClass="M" unitType="Mek"
					weight="3">%TRINARY%</option>
				<option weightClass="L" unitType="Mek"
					weight="2">%TRINARY%</option>
				<option weightClass="H" unitType="Mek" augmented="1"
					ifDateBetween="2870," ifRating="FL|Keshik"
					weight="4">%TRINARY%</option>
				<option weightClass="M" unitType="Mek" augmented="1"
					ifDateBetween="2870," ifRating="FL|Keshik"
					weight="6">%TRINARY%</option>
				<option weightClass="L" unitType="Mek" augmented="1"
					ifDateBetween="2870," ifRating="FL|Keshik"
					weight="4">%TRINARY%</option>
				<option weightClass="H" unitType="Mek" augmented="1"
					ifDateBetween="2900," ifRating="SL"
					weight="2">%TRINARY%</option>
				<option weightClass="M" unitType="Mek" augmented="1"
					ifDateBetween="2900," ifRating="SL"
					weight="3">%TRINARY%</option>
				<option weightClass="L" unitType="Mek" augmented="1"
					ifDateBetween="2900," ifRating="SL"
					weight="2">%TRINARY%</option>
			</subforceOption>
			<subforceOption ifWeightClass="L">
				<option weightClass="M" unitType="Mek"
					weight="1">%TRINARY%</option>
				<option weightClass="L" unitType="Mek"
					weight="2">%TRINARY%</option>
				<option weightClass="M" unitType="Mek" augmented="1"
					ifDateBetween="2870," ifRating="FL|Keshik"
					weight="2">%TRINARY%</option>
				<option weightClass="L" unitType="Mek" augmented="1"
					ifDateBetween="2870," ifRating="FL|Keshik"
					weight="4">%TRINARY%</option>
				<option weightClass="M" unitType="Mek" augmented="1"
					ifDateBetween="2900," ifRating="SL"
					weight="1">%TRINARY%</option>
				<option weightClass="L" unitType="Mek" augmented="1"
					ifDateBetween="2900," ifRating="SL"
					weight="2">%TRINARY%</option>
			</subforceOption>

			<subforceOption>
				<option ifDateBetween="2870," unitType="BattleArmor"  
					ifRating="FL|Keshik">%TRINARY%</option>
				<option ifDateBetween="2900," unitType="BattleArmor"  
					ifRating="SL">%TRINARY%</option>
				<option ifRating="SL" unitType="Infantry"
					ifDateBetween=",2950">%TRINARY%</option>
				<option ifRating="PG|Sol"
					unitType="Infantry">%TRINARY%</option>
			</subforceOption>

			<subforceOption ifWeightClass="H|A">
				<option weightClass="H" unitType="Aero"
					weight="3">%TRINARY%</option>
				<option weightClass="M" unitType="Aero"
					weight="2">%TRINARY%</option>
				<option weightClass="L" unitType="Aero"
					weight="1">%TRINARY%</option>
			</subforceOption>
			<subforceOption ifWeightClass="M">
				<option weightClass="H" unitType="Aero"
					weight="2">%TRINARY%</option>
				<option weightClass="M" unitType="Aero"
					weight="3">%TRINARY%</option>
				<option weightClass="L" unitType="Aero"
					weight="2">%TRINARY%</option>
			</subforceOption>
			<subforceOption ifWeightClass="L">
				<option weightClass="H" unitType="Aero"
					weight="1">%TRINARY%</option>
				<option weightClass="M" unitType="Aero"
					weight="2">%TRINARY%</option>
				<option weightClass="L" unitType="Aero"
					weight="3">%TRINARY%</option>
			</subforceOption>
		</subforces>
		
		<!--Fifth cluster has equal chance of being Mek or ASF, except
		in combined clusters where the chance is 1:2.-->
		
		<subforces ifRating="Keshik|FL|SL">
			<subforceOption ifWeightClass="A">
				<option weightClass="A" unitType="Mek"
					weight="4">%TRINARY%</option>
				<option weightClass="H" unitType="Mek"
					weight="2">%TRINARY%</option>
				<option weightClass="H" unitType="Aero"
					weight="3">%TRINARY%</option>
				<option weightClass="M" unitType="Aero"
					weight="2">%TRINARY%</option>
				<option weightClass="L" unitType="Aero"
					weight="1">%TRINARY%</option>
				<option weightClass="H" unitType="Aero"
					ifFlags="combined" weight="3">%TRINARY%</option>
				<option weightClass="M" unitType="Aero"
					ifFlags="combined" weight="2">%TRINARY%</option>
				<option weightClass="L" unitType="Aero"
					ifFlags="combined" weight="1">%TRINARY%</option>
			</subforceOption>
			<subforceOption ifWeightClass="H">
				<option weightClass="A" unitType="Mek"
					weight="2">%TRINARY%</option>
				<option weightClass="H" unitType="Mek"
					weight="3">%TRINARY%</option>
				<option weightClass="M" unitType="Mek"
					weight="2">%TRINARY%</option>
				<option weightClass="H" unitType="Aero"
					weight="3">%TRINARY%</option>
				<option weightClass="M" unitType="Aero"
					weight="2">%TRINARY%</option>
				<option weightClass="L" unitType="Aero"
					weight="1">%TRINARY%</option>
				<option weightClass="H" unitType="Aero"
					ifFlags="combined" weight="3">%TRINARY%</option>
				<option weightClass="M" unitType="Aero"
					ifFlags="combined" weight="2">%TRINARY%</option>
				<option weightClass="L" unitType="Aero"
					ifFlags="combined" weight="1">%TRINARY%</option>
			</subforceOption>
			<subforceOption ifWeightClass="M">
				<option weightClass="H" unitType="Mek"
					weight="2">%TRINARY%</option>
				<option weightClass="M" unitType="Mek"
					weight="3">%TRINARY%</option>
				<option weightClass="L" unitType="Mek"
					weight="2">%TRINARY%</option>
				<option weightClass="H" unitType="Aero"
					weight="2">%TRINARY%</option>
				<option weightClass="M" unitType="Aero"
					weight="3">%TRINARY%</option>
				<option weightClass="L" unitType="Aero"
					weight="2">%TRINARY%</option>
				<option weightClass="H" unitType="Aero"
					ifFlags="combined" weight="2">%TRINARY%</option>
				<option weightClass="M" unitType="Aero"
					ifFlags="combined" weight="3">%TRINARY%</option>
				<option weightClass="L" unitType="Aero"
					ifFlags="combined" weight="2">%TRINARY%</option>
			</subforceOption>
			<subforceOption ifWeightClass="L">
				<option weightClass="M" unitType="Mek"
					weight="2">%TRINARY%</option>
				<option weightClass="L" unitType="Mek"
					weight="4">%TRINARY%</option>
				<option weightClass="H" unitType="Aero"
					weight="1">%TRINARY%</option>
				<option weightClass="M" unitType="Aero"
					weight="2">%TRINARY%</option>
				<option weightClass="L" unitType="Aero"
					weight="3">%TRINARY%</option>
				<option weightClass="H" unitType="Aero"
