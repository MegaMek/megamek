<?xml version="1.0" encoding="utf-8"?>

<ruleset faction="CSA" ratingSystem="CLAN" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="formationRulesetSchema.xsd">

	<defaults>
		<eschelon>%CLUSTER%</eschelon>
		<rankSystem>%RANK_CLAN%</rankSystem>
		<rating>FL</rating>
	</defaults>
	
	<toc>
		<unitType>
			<option ifDateBetween="3060,">null,Mek,ProtoMek,Tank,VTOL,Infantry,BattleArmor,AeroSpaceFighter,Conventional Fighter,Warship,Dropship,Jumpship</option>
			<option ifDateBetween="2870,3059">null,Mek,Tank,VTOL,Infantry,BattleArmor,AeroSpaceFighter,Conventional Fighter,Warship,Dropship,Jumpship</option>
			<option ifDateBetween=",2869">null,Mek,Tank,VTOL,Infantry,AeroSpaceFighter,Conventional Fighter,Warship,Dropship,Jumpship</option>
		</unitType>
		
		<eschelon>
			<option ifUnitType="">%TOUMAN%,%GALAXY%,%CLUSTER%</option>
			<option ifUnitType="Warship">%CLUSTER%</option>
			<option ifUnitType="Dropship|Jumpship">%STAR%</option>
			<option ifUnitType="Mek" ifDateBetween="2870,">%TRINARY%^,%TRINARY%,%BINARY%^,%BINARY%,%STAR%^,%STAR%</option>
			<option>%TRINARY%,%BINARY%,%STAR%</option>
		</eschelon>
		
		<rating>
			<option ifUnitType="Tank|VTOL">SL:Second Line,PG:Provisional Garrison,Sol:Solahma</option>
			<option ifUnitType="Infantry" ifDateBetween="2900,">PG:Provisional Garrison,Sol:Solahma</option>
			<option ifUnitType="Infantry">SL:Second Line,PG:Provisional Garrison,Sol:Solahma</option>
			<option ifEschelon="%TOUMAN%"></option>
			<option ifEschelon="%GALAXY%">FL:Front Line,SL:Second Line</option>
			<option>Keshik,FL:Front Line,SL:Second Line,PG:Provisional Garrison,Sol:Solahma</option>
		</rating>
		
		<flags>
			<option ifEschelon="%CLUSTER%">assault:Assault Cluster,cavalry:Cavalry Cluster,cavalier:Cavalier Cluster,dragoon:Dragoon Cluster,guards:Guards Cluster,hussar:Hussar Cluster</option>
		</flags>
	</toc>
	
	<force eschelon="%TOUMAN%" eschName="Touman">
		<co>%KHAN%</co>
		<xo>%SAKHAN%</xo>
		
		<!--Some of the dates below are ballpark guesses.-->
		<subforces>
			<subforce rating="Keshik" role="command" ifDateBetween="2870,"
				name="Adder Command Keshik">%CLUSTER%</subforce>
			<subforce rating="Keshik" ifDateBetween="2870,"
				name="Adder Quasar Keshik">%CLUSTER%</subforce>
			<subforce rating="FL" flags="alpha"
				name="Alpha Galaxy">%GALAXY%</subforce>
			<subforce rating="FL" ifDateBetween=",3059|3062," flags="beta"
				name="Beta Galaxy">%GALAXY%</subforce>
			<subforce rating="FL" ifDateBetween="2920,"
				name="Gamma Galaxy">%GALAXY%</subforce>
			<subforce rating="FL" ifDateBetween="2938,"
				name="Delta Galaxy">%GALAXY%</subforce>
			<subforce rating="FL" ifDateBetween="3023,"
				name="Epsilon Galaxy">%GALAXY%</subforce>
			<subforce rating="SL"
				name="Kappa Provisional Galaxy">%GALAXY%</subforce>
			<subforce rating="SL" ifDateBetween="2860,"
				name="Mu Provisional Galaxy">%GALAXY%</subforce>
			<subforce rating="SL" ifDateBetween="2880,2970"
				name="Nu Provisional Galaxy">%GALAXY%</subforce>
			<subforce rating="SL" ifDateBetween="2966,"
				name="Xi Provisional Galaxy">%GALAXY%</subforce>
			<subforce rating="SL" ifDateBetween="2970,"
				name="Omicron Provisional Galaxy">%GALAXY%</subforce>
			<subforce rating="SL" ifDateBetween="2990,3017|3058,"
				name="Rho Provisional Galaxy">%GALAXY%</subforce>
			<subforce rating="SL" ifDateBetween="2998,"
				name="Sigma Provisional Galaxy">%GALAXY%</subforce>
			<subforce rating="SL" ifDateBetween="3059,"
				name="Tau Provisional Galaxy">%GALAXY%</subforce>
			<subforce rating="SL" ifDateBetween="3059,3059|3062,3072"
				name="Upsilon Provisional Galaxy">%GALAXY%</subforce>
		</subforces>
		
		<attachedForces>
			<subforce name="Naval Reserve"
				unitType="Warship">%CLUSTER%</subforce>
		</attachedForces>
	</force>
	
	<force eschelon="%GALAXY%" eschName="Galaxy">
		<name>{greek} Galaxy</name>
		<co>%GALAXY_CMDR%</co>
		
		<subforces>
			<subforceOption>
				<option role="command" unitType="Mek"
					weight="2">%TRINARY%</option>
				<option role="command" unitType="Mek" ifDateBetween="2870,"
					augmented="1" weight="1">%TRINARY%</option>
			</subforceOption>
		</subforces>
		<subforces ifRating="FL">
			<subforce flags="+assault">%CLUSTER%</subforce>
			<subforce num="4">%CLUSTER%</subforce>
		</subforces>
		<subforces ifRating="SL">
			<subforce num="3">%CLUSTER%</subforce>
			<subforceOption>
				<option>%CLUSTER%</option>
				<option rating="Sol">%CLUSTER%</option>
			</subforceOption>
			<subforceOption>
				<option>%CLUSTER%</option>
				<option rating="Sol">%CLUSTER%</option>
				<option />
			</subforceOption>
		</subforces>

		<attachedForces ifRating="SL|PG">
			<subforceOption>
				<option num="2" rating="Sol"
					unitType="Tank">%CLUSTER%</option>
				<option num="1" rating="Sol"
					unitType="Tank">%CLUSTER%</option>
				<option />
			</subforceOption>
		</attachedForces>
	</force>
	
	<force eschelon="%CLUSTER%" eschName="Fleet" ifUnitType="Warship">
		<name>Naval Reserve</name>
		<co>%GALAXY_CMDR%</co>
		
		<subforces>
			<subforce chassis="Sovetskii Soyuz Heavy Cruiser"
				ifDateBetween=",3075" role="command"
				fluffName="CSA Nygaard">%ELEMENT%</subforce>
			<subforce chassis="Aegis Heavy Cruiser"
				fluffName="CSA Stellar Serpent">%ELEMENT%</subforce>
			<subforce chassis="McKenna Battleship"
				fluffName="CSA Sovereign Right">%ELEMENT%</subforce>
			<subforce chassis="Black Lion II Battlecruiser"
				fluffName="CSA Admiral William S. Preston"
				ifDateBetween=",3072">%ELEMENT%</subforce>
			<subforce chassis="Liberator Cruiser" ifDateBetween="3059,3068"
				fluffName="CSA Constantineau">%ELEMENT%</subforce>
			<subforce chassis="Aegis Heavy Cruiser"
				fluffName="CSA Ares&apos;s Might">%ELEMENT%</subforce>
			<subforce fluffName="CSA Absolute Truth" ifDateBetween="2932,"
				chassis="Nightlord Battleship">%ELEMENT%</subforce>
			<subforce fluffName="CSA Exodus Ranger" 
				ifDateBetween="2950,3075"
				chassis="York Destroyer-Carrier">%ELEMENT%</subforce>
			<subforce fluffName="CSA Hagar"
				chassis="Lola III Destroyer">%ELEMENT%</subforce>
			<subforce fluffName="CSA Vritra" ifDateBetween=",3067"
				chassis="Vincent Corvette">%ELEMENT%</subforce>
			<subforce fluffName="CSA Centaur"
				chassis="Vincent Corvette">%ELEMENT%</subforce>
			<subforce fluffName="CSA Divine Conquest" ifDateBetween=",3075"
				chassis="Sovetskii Soyuz Heavy Cruiser">%ELEMENT%</subforce>
			<subforce fluffName="CSA Star Fire" ifDateBetween="2950,3060"
				chassis="York Destroyer-Carrier">%ELEMENT%</subforce>
			<subforce fluffName="CSA Eagle" ifDateBetween=",3072"
				chassis="Essex II Destroyer">%ELEMENT%</subforce>
			<subforce fluffName="CSA Vicious Fang" ifDateBetween="2962,"
				chassis="Fredasa (Corvette-Raider)">%ELEMENT%</subforce>
			<subforce fluffName="CSA Black Adder" ifDateBetween="2962,3072"
				chassis="Carrack Transport">%ELEMENT%</subforce>
			<subforce fluffName="CSA Exodus Sentinel" ifDateBetween="2950,"
				chassis="York Destroyer-Carrier">%ELEMENT%</subforce>
			<subforce fluffName="CSA Warlock"
				chassis="Lola III Destroyer">%ELEMENT%</subforce>
			<subforce fluffName="CSA Yodan" ifDateBetween=",3072"
				chassis="Lola III Destroyer">%ELEMENT%</subforce>
			<subforce fluffName="CSA Tehuantepec" ifDateBetween=",3072"
				chassis="Essex II Destroyer">%ELEMENT%</subforce>
			<subforce fluffName="CSA Arcadian Asp" ifDateBetween="2962,"
				chassis="Fredasa (Corvette-Raider)">%ELEMENT%</subforce>
			<subforce fluffName="CSA Exodus Crusader" ifDateBetween="2950,"
				chassis="York Destroyer-Carrier">%ELEMENT%</subforce>
			<subforce fluffName="CSA Exodus Avenger" ifDateBetween="2950,"
				chassis="York Destroyer-Carrier">%ELEMENT%</subforce>
			<subforce fluffName="CSA Pegasus" ifDateBetween=",3088"
				chassis="Vincent Corvette">%ELEMENT%</subforce>
			<subforce fluffName="CSA Cho Polu" ifDateBetween=",3068"
				chassis="Volga Transport">%ELEMENT%</subforce>
			<subforce fluffName="CSA Pompeii" ifDateBetween=",3072"
				chassis="Volga Transport">%ELEMENT%</subforce>
			<subforce fluffName="CSA Renown" ifDateBetween=",3062"
				chassis="Potemkin Troop Cruiser">%ELEMENT%</subforce>
			<subforce fluffName="CSA Cameron&apos; Flame" 
				ifDateBetween=",3072"
				chassis="Lola III Destroyer">%ELEMENT%</subforce>
		</subforces>
	</force>
	
	<!--Solahma cluster based around vehicles; these do not appear on the
	TO&E, so are attached to SL galaxies. 2-3 Tank binaries/trinaries+
	1-2 conventional infantry binaries/trinaries+1 ASF binary.-->
	
	<force eschelon="%CLUSTER%" eschName="Cluster" ifUnitType="Tank">
		<name>Gatekeeper Cluster</name>
		<co>%STAR_COL%</co>

		<weightClass>
			<option weight="3">H</option>
			<option>M</option>
			<option>L</option>
			<option ifDateBetween="3060,">L</option>
		</weightClass>

		<subforces>
			<subforceOption ifWeightClass="H|A">
				<option weightClass="A,A" unitType="Tank"
					flags="+testTrinary" weight="5">%TRINARY%</option>
				<option weightClass="A,H" unitType="Tank"
					flags="+testTrinary" weight="5">%TRINARY%</option>
				<option weightClass="H,H" unitType="Tank"
					flags="+testTrinary" weight="5">%TRINARY%</option>
			</subforceOption>
			<subforceOption ifWeightClass="M">
				<option weightClass="H,M" unitType="Tank"
					flags="+testTrinary" weight="5">%TRINARY%</option>
				<option weightClass="H,L" unitType="Tank"
					flags="+testTrinary" weight="5">%TRINARY%</option>
				<option weightClass="M,M" unitType="Tank"
					flags="+testTrinary" weight="5">%TRINARY%</option>
			</subforceOption>
			<subforceOption ifWeightClass="L">
				<option weightClass="M,M" unitType="Tank"
					flags="+testTrinary" weight="5">%TRINARY%</option>
				<option weightClass="M,L" unitType="Tank"
					flags="+testTrinary" weight="5">%TRINARY%</option>
				<option weightClass="L,L" unitType="Tank"
					flags="+testTrinary" weight="5">%TRINARY%</option>
			</subforceOption>
			
			<subforceOption ifWeightClass="A|H">
				<option weightClass="A" unitType="Tank"
					flags="+testTrinary" weight="2">%TRINARY%</option>
				<option weightClass="H" unitType="Tank"
					flags="+testTrinary" weight="2">%TRINARY%</option>
			</subforceOption>
			<subforceOption ifWeightClass="M">
				<option weightClass="H" unitType="Tank"
					flags="+testTrinary" weight="2">%TRINARY%</option>
				<option weightClass="M" unitType="Tank"
					flags="+testTrinary" weight="2">%TRINARY%</option>
			</subforceOption>
			<subforceOption ifWeightClass="L">
				<option weightClass="M" unitType="Tank"
					flags="+testTrinary" weight="2">%TRINARY%</option>
				<option weightClass="L" unitType="Tank"
					flags="+testTrinary" weight="2">%TRINARY%</option>
			</subforceOption>

			<subforceOption>
				<option unitType="Infantry">%TRINARY%</option>
				<option unitType="Infantry">%BINARY%</option>
			</subforceOption>
			<subforceOption>
				<option unitType="Infantry">%TRINARY%</option>
				<option unitType="Infantry">%BINARY%</option>
				<option weight="2" />
			</subforceOption>
			
			<subforceOption>
				<option unitType="AeroSpaceFighter" weightClass="H"
					weight="3">%BINARY%</option>
				<option unitType="AeroSpaceFighter" weightClass="M"
					weight="2">%BINARY%</option>
				<option unitType="AeroSpaceFighter" weightClass="L">%BINARY%</option>
			</subforceOption>		
		</subforces>
	</force>

	<force eschelon="%CLUSTER%" eschName="Cluster">
		<name ifFlags="cavalry">Armored Cavalry Squadron</name>
		<name ifFlags="assault">Assault Cluster</name>
		<name ifFlags="cavalier">Adder Cavaliers</name>
		<name ifFlags="dragoon">Dragoon Cluster</name>
		<name ifFlags="guards">Adder Guards</name>
		<name ifFlags="hussar">Hussar Cluster</name>
		<name ifRating="FL" ifWeightClass="A|H">Assault Cluster</name>
		<name ifRating="FL" ifWeightClass="M">Battle Cluster</name>
		<name ifRating="FL" ifWeightClass="L">Striker Cluster</name>
		<name ifRating="SL">Adder Sentinels</name>
		<name ifRating="Sol">Gatekeeper Cluster</name>
		<co>%STAR_COL%</co>
		
		<!--Distribution is taken from FM:U, after recovery from 
		Burrock absorption. -->
		
		<flags ifRating="FL" ifFlags="!assault|cavalry|cavalier|dragoon|guards|hussar">
			<option weight="5" weightClass="H"
				ifTopLevel="1">assault</option>
			<option weight="5" weightClass="M">cavalry</option>
			<option weight="4" role="cavalry">cavalier</option>
			<option weight="4">dragoon</option>
			<option weight="3" weightClass="H">guards</option>
			<option weight="1" weightClass="L"
				role="cavalry">hussar</option>
			<option weight="1" ifDateBetween="3060,"
				weightClass="L" role="cavalry">hussar</option>
		</flags>
		
		<ruleGroup>
			<weightClass>
				<option weight="3">H</option>
				<option weight="2" ifRole="!command">M</option>
				<option weight="1" ifRole="!command"
					ifDateBetween="3060,">L</option>
			</weightClass>
		</ruleGroup>
		
		<!--Armored cavalry squadrons have M and H Meks with a few
		stars of light scouts and four stars of ASF.-->
		<subforces ifFlags="cavalry">
			<subforceOption>
				<option weightClass="A" role="command"
					unitType="Mek">%TRINARY%</option>
				<option weightClass="H" role="command"
					unitType="Mek" weight="3">%TRINARY%</option>
				<option weightClass="M" role="command"
					unitType="Mek">%TRINARY%</option>
			</subforceOption>
			<subforceOption>
				<option weightClass="A,H"
					unitType="Mek">%TRINARY%</option>
				<option weightClass="A,M"
					unitType="Mek">%TRINARY%</option>
				<option weightClass="H,H" weight="2"
					unitType="Mek">%TRINARY%</option>
				<option weightClass="H,M" weight="2"
					unitType="Mek">%TRINARY%</option>
				<option weightClass="H,L"
					unitType="Mek">%TRINARY%</option>
				<option weightClass="M,M"
					unitType="Mek">%TRINARY%</option>
			</subforceOption>
			<subforceOption>
				<option unitType="AeroSpaceFighter" weightClass="H">%TRINARY%+</option>
				<option unitType="AeroSpaceFighter" weightClass="M">%TRINARY%+</option>
				<option unitType="AeroSpaceFighter" weightClass="L">%TRINARY%+</option>
			</subforceOption>		
		</subforces>
		
		<!--Assault clusters have H and A Meks with one of the trinaries
		including some of the faster heavies.-->
		<subforces ifFlags="assault">
			<subforceOption>
				<option weightClass="A" unitType="Mek"
					role="command" weight="2">%TRINARY%</option>
				<option weightClass="H" unitType="Mek"
					role="command">%TRINARY%</option>
			</subforceOption>
			<subforceOption>
				<option weightClass="A" unitType="Mek"
					role="command">%TRINARY%</option>
				<option weightClass="H" unitType="Mek"
					role="command">%TRINARY%</option>
			</subforceOption>
			<subforce unitType="Mek" weightClass="H"
				role="cavalry">%TRINARY%</subforce>
		</subforces>
		
		<!--Remaining cluster types can be dealt with together in
		Mek weight distribution:
		-Cavalier speed with hitting power, indicating that they are
		faster than the typical heavy-hitting Adder Meks. This is
		handled by using the cavalry role, added above.
		-Dragoon emphasize flexibility, which is handled by ensuring
		a greater variation in trinary weight classes.
		-Guards are heavy clusters with a lower chance of BA, dealt
		with in the BA rule below.
		-Hussars are fast M and L with a few H stars, handled by ensuring
		they are L clusters and adding the cavalry role above and making
		sure there is at least one M trinary.
		
		This also takes care of generic battle and striker clusters
		that appear in the TO&E.
		-->
		
		<subforces ifRating="FL|Keshik"
				ifFlags="!cavalry|assault">
			<subforceOption ifWeightClass="H|A">
				<option weightClass="A" unitType="Mek"
					role="command">%TRINARY%</option>
				<option weightClass="H" unitType="Mek"
					role="command">%TRINARY%</option>
			</subforceOption>
			<subforceOption ifWeightClass="M">
				<option weightClass="H" unitType="Mek"
					role="command">%TRINARY%</option>
				<option weightClass="M" unitType="Mek"
					role="command">%TRINARY%</option>
			</subforceOption>
			<subforceOption ifWeightClass="L">
				<option weightClass="M" unitType="Mek"
					role="command" weight="3">%TRINARY%</option>
				<option weightClass="L" unitType="Mek"
					role="command">%TRINARY%</option>
			</subforceOption>
				
			<subforceOption ifWeightClass="H|A">
				<option weightClass="A,A" ifFlags="!dragoon"
					unitType="Mek">%TRINARY%</option>
				<option weightClass="A,H"
					unitType="Mek">%TRINARY%</option>
				<option weightClass="A,H" ifFlags="dragoon"
					unitType="Mek">%TRINARY%</option>
				<option weightClass="A,M" ifFlags="dragoon"
					unitType="Mek">%TRINARY%</option>
				<option weightClass="A,L" ifFlags="dragoon"
					unitType="Mek">%TRINARY%</option>
				<option weightClass="H,H"
					unitType="Mek">%TRINARY%</option>
				<option weightClass="H,M"
					unitType="Mek">%TRINARY%</option>
				<option weightClass="H,M" ifFlags="dragoon"
					unitType="Mek">%TRINARY%</option>
			</subforceOption>
			<subforceOption ifWeightClass="M">
				<option weightClass="H,M" ifFlags="dragoon"
					unitType="Mek">%TRINARY%</option>
				<option weightClass="H,M"
					unitType="Mek">%TRINARY%</option>
				<option weightClass="H,L" ifFlags="dragoon"
					unitType="Mek">%TRINARY%</option>
				<option weightClass="M,M"
					unitType="Mek">%TRINARY%</option>
				<option weightClass="M,L"
					unitType="Mek">%TRINARY%</option>
				<option weightClass="M,L" ifFlags="dragoon"
					unitType="Mek">%TRINARY%</option>
			</subforceOption>
			<subforceOption ifWeightClass="L">
				<option weightClass="M,M"
					unitType="Mek">%TRINARY%</option>
				<option weightClass="M,L"
					unitType="Mek">%TRINARY%</option>
				<option weightClass="M,L" ifFlags="dragoon"
					unitType="Mek">%TRINARY%</option>
			</subforceOption>			
		</subforces>
		
		<!--Possible binary or trinary of BA or supernova,
		and 1-3 stars of ASF-->

		<subforces ifRating="FL|Keshik">
			<subforceOption ifDateBetween="2870,">
				<option unitType="BattleArmor"
					weight="2">%TRINARY%</option>
				<option unitType="BattleArmor"
					weight="3">%BINARY%</option>
				<option unitType="Mek" augmented="1">%TRINARY%</option>
				<option unitType="Mek" augmented="1">%BINARY%</option>
				<option ifDateBetween=",3060" weight="4"/>
				<option ifFlags="guards|sentinels" weight="8" />
			</subforceOption>
			<subforceOption ifFlags="!cavalry">
				<option unitType="AeroSpaceFighter" weightClass="H"
					weight="3">%TRINARY%</option>
				<option unitType="AeroSpaceFighter" weightClass="M"
					weight="2">%TRINARY%</option>
				<option unitType="AeroSpaceFighter" weightClass="L">%TRINARY%</option>
				<option unitType="AeroSpaceFighter" weightClass="H"
					weight="3">%BINARY%</option>
				<option unitType="AeroSpaceFighter" weightClass="M"
					weight="2">%BINARY%</option>
				<option unitType="AeroSpaceFighter" weightClass="L">%BINARY%</option>
				<option unitType="AeroSpaceFighter" weightClass="H"
					weight="3">%STAR%</option>
				<option unitType="AeroSpaceFighter" weightClass="M"
					weight="2">%STAR%</option>
				<option unitType="AeroSpaceFighter" weightClass="L">%STAR%</option>
			</subforceOption>		
		</subforces>

		<!--Sentinel (SL) clusters vary quite a bit, but are centered
		around Meks, often with a binary or trinary of BA and 1-2 stars
		of ASF. They also include vehicles and conventional infantry.
		We give them two Mek binaries or trinaries, one vehicle,
		and a fourth that could be Mek or vehicle. Gatekeeper
		(solahma) clusters are centered either around Meks or around
		vehicles and conventional infantry, but only the former appear
		on the TO&E. They do not have BA.--> 

		<subforces ifRating="SL|PG|Sol">
			<subforceOption ifWeightClass="H|A">
				<option weightClass="A,A" unitType="Mek"
					flags="+testTrinary" weight="5">%TRINARY%</option>
				<option weightClass="A,H" unitType="Mek"
					flags="+testTrinary" weight="5">%TRINARY%</option>
				<option weightClass="H,H" unitType="Mek"
					flags="+testTrinary" weight="5">%TRINARY%</option>
			</subforceOption>
			<subforceOption ifWeightClass="M">
				<option weightClass="H,M" unitType="Mek"
					flags="+testTrinary" weight="5">%TRINARY%</option>
				<option weightClass="H,L" unitType="Mek"
					flags="+testTrinary" weight="5">%TRINARY%</option>
				<option weightClass="M,M" unitType="Mek"
